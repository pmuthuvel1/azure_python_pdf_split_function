{
  "scriptFile": "main.py",
  "bindings": [
    {
      "name": "myblob",
      "type": "blobTrigger",
      "direction": "in",
      "path": "pdf-input/{name}",
      "connection": "pdfsplitstorage1_STORAGE"
    },
    {
      "name": "outputblob",
      "type": "blob",
